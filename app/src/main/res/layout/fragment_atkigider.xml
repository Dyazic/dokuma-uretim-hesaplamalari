<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AtkiGiderFragment">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="SpeakableTextPresentCheck">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="15dp"
                android:text="Toplam Adım Sayısı= "
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/buttonHesaplaA" />

            <TextView
                android:id="@+id/textViewToplamRaporBoyuA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                app:layout_constraintStart_toEndOf="@+id/textView24"
                app:layout_constraintTop_toTopOf="@+id/textView24" />

            <TextView
                android:id="@+id/textView25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="20dp"
                android:text="1. İplik Gider ="
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView24" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="40dp"
                android:text="5. İplik Gider="
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/textView28"
                app:layout_constraintTop_toBottomOf="@+id/textView28" />

            <TextView
                android:id="@+id/textViewBirinciGiderA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                app:layout_constraintStart_toEndOf="@+id/textView25"
                app:layout_constraintTop_toTopOf="@+id/textView25" />

            <TextView
                android:id="@+id/textView26"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="2. İplik Gider ="
                app:layout_constraintStart_toStartOf="@+id/textView25"
                app:layout_constraintTop_toBottomOf="@+id/textView25" />

            <TextView
                android:id="@+id/textViewIkinciGiderA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="@+id/textViewBirinciGiderA"
                app:layout_constraintTop_toTopOf="@+id/textView26" />

            <TextView
                android:id="@+id/textView27"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="3. İplik Gider ="
                app:layout_constraintStart_toStartOf="@+id/textView26"
                app:layout_constraintTop_toBottomOf="@+id/textView26" />

            <TextView
                android:id="@+id/textViewUcuncuGiderA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="@+id/textViewIkinciGiderA"
                app:layout_constraintTop_toTopOf="@+id/textView27" />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="4. İplik Gider ="
                app:layout_constraintStart_toStartOf="@+id/textView27"
                app:layout_constraintTop_toBottomOf="@+id/textView27" />

            <TextView
                android:id="@+id/textViewDorduncuGiderA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="@+id/textViewUcuncuGiderA"
                app:layout_constraintTop_toTopOf="@+id/textView28" />

            <TextView
                android:id="@+id/textViewBesinciGiderA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="@+id/textViewDorduncuGiderA"
                app:layout_constraintTop_toTopOf="@+id/textView29" />

            <EditText
                android:id="@+id/editTextIplikBirA"
                android:layout_width="90dp"
                android:layout_height="40dp"
                android:layout_marginTop="20dp"
                android:ems="10"
                android:hint="1.İplik No"
                android:inputType="number|numberDecimal"
                app:layout_constraintStart_toStartOf="@+id/textView22"
                app:layout_constraintTop_toBottomOf="@+id/textView22"
                tools:ignore="TouchTargetSizeCheck" />

            <EditText
                android:id="@+id/editTextIplikIkiA"
                android:layout_width="90dp"
                android:layout_height="40dp"
                android:layout_marginTop="20dp"
                android:ems="10"
                android:hint="2.İplik No"
                android:inputType="number|numberDecimal"
                app:layout_constraintStart_toStartOf="@+id/editTextIplikBirA"
                app:layout_constraintTop_toBottomOf="@+id/editTextIplikBirA" />

            <EditText
                android:id="@+id/editTextIplikUcA"
                android:layout_width="90dp"
                android:layout_height="40dp"
                android:layout_marginTop="20dp"
                android:ems="10"
                android:hint="3.iplik No"
                android:inputType="number|numberDecimal"
                app:layout_constraintStart_toStartOf="@+id/editTextIplikIkiA"
                app:layout_constraintTop_toBottomOf="@+id/editTextIplikIkiA" />

            <EditText
                android:id="@+id/editTextIplikDortA"
                android:layout_width="90dp"
                android:layout_height="40dp"
                android:layout_marginTop="20dp"
                android:ems="10"
                android:hint="4.İplik No"
                android:inputType="number|numberDecimal"
                app:layout_constraintStart_toStartOf="@+id/editTextIplikUcA"
                app:layout_constraintTop_toBottomOf="@+id/editTextIplikUcA"
                tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,TouchTargetSizeCheck" />

            <EditText
                android:id="@+id/editTextIplikBesA"
                android:layout_width="90dp"
                android:layout_height="40dp"
                android:layout_marginTop="20dp"
                android:ems="10"
                android:hint="5.İplik No"
                android:inputType="number|numberDecimal"
                app:layout_constraintStart_toStartOf="@+id/editTextIplikDortA"
                app:layout_constraintTop_toBottomOf="@+id/editTextIplikDortA"
                tools:ignore="TouchTargetSizeCheck" />

            <TextView
                android:id="@+id/textView22"
                android:layout_width="87dp"
                android:layout_height="35dp"
                android:layout_marginStart="30dp"
                android:layout_marginTop="60dp"
                android:text="İPLİK NO ALANI"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextSiklikA" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="53dp"
                android:layout_height="40dp"
                android:layout_marginStart="20dp"
                android:text="RAPOR ALANI"
                app:layout_constraintStart_toEndOf="@+id/textViewBirIplikA"
                app:layout_constraintTop_toTopOf="@+id/textView22" />

            <TextView
                android:id="@+id/textViewBirIplikA"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_marginStart="20dp"
                app:layout_constraintStart_toEndOf="@+id/editTextIplikBirA"
                app:layout_constraintTop_toTopOf="@+id/editTextIplikBirA" />

            <EditText
                android:id="@+id/editTextIpikRaporBirA"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:ems="10"
                android:inputType="number|numberDecimal"
                android:text="0"
                app:layout_constraintStart_toStartOf="@+id/textView23"
                app:layout_constraintTop_toTopOf="@+id/textViewBirIplikA" />

            <TextView
                android:id="@+id/textViewIkiIplikA"
                android:layout_width="50dp"
                android:layout_height="40dp"
                app:layout_constraintStart_toStartOf="@+id/textViewBirIplikA"
                app:layout_constraintTop_toTopOf="@+id/editTextIplikIkiA" />

            <EditText
                android:id="@+id/editTextIpikRaporIkiA"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:ems="10"
                android:inputType="number|numberDecimal"
                android:text="0"
                app:layout_constraintStart_toStartOf="@+id/editTextIpikRaporBirA"
                app:layout_constraintTop_toTopOf="@+id/textViewIkiIplikA"
                tools:ignore="TouchTargetSizeCheck" />

            <TextView
                android:id="@+id/textViewUcIplikA"
                android:layout_width="50dp"
                android:layout_height="40dp"
                app:layout_constraintStart_toStartOf="@+id/textViewIkiIplikA"
                app:layout_constraintTop_toTopOf="@+id/editTextIplikUcA" />

            <EditText
                android:id="@+id/editTextIpikRaporUcA"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:ems="10"
                android:inputType="number|numberDecimal"
                android:text="0"
                app:layout_constraintStart_toStartOf="@+id/editTextIpikRaporIkiA"
                app:layout_constraintTop_toTopOf="@+id/textViewUcIplikA"
                tools:ignore="TouchTargetSizeCheck" />

            <TextView
                android:id="@+id/textViewDortIplikA"
                android:layout_width="50dp"
                android:layout_height="40dp"
                app:layout_constraintStart_toStartOf="@+id/textViewUcIplikA"
                app:layout_constraintTop_toTopOf="@+id/editTextIplikDortA" />

            <EditText
                android:id="@+id/editTextIpikRaporDortA"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:ems="10"
                android:inputType="number|numberDecimal"
                android:text="0"
                app:layout_constraintStart_toStartOf="@+id/editTextIpikRaporUcA"
                app:layout_constraintTop_toTopOf="@+id/textViewDortIplikA" />

            <TextView
                android:id="@+id/textViewBesIplikA"
                android:layout_width="50dp"
                android:layout_height="40dp"
                app:layout_constraintStart_toStartOf="@+id/textViewDortIplikA"
                app:layout_constraintTop_toTopOf="@+id/editTextIplikBesA" />

            <EditText
                android:id="@+id/editTextIpikRaporBesA"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:ems="10"
                android:inputType="number|numberDecimal"
                android:text="0"
                app:layout_constraintStart_toStartOf="@+id/editTextIpikRaporDortA"
                app:layout_constraintTop_toTopOf="@+id/textViewBesIplikA"
                tools:ignore="TouchTargetSizeCheck" />

            <Button
                android:id="@+id/buttonPopupCozBirA"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginStart="20dp"
                app:icon="@android:drawable/arrow_down_float"
                app:layout_constraintStart_toEndOf="@+id/editTextIpikRaporBirA"
                app:layout_constraintTop_toTopOf="@+id/editTextIpikRaporBirA" />

            <Button
                android:id="@+id/buttonPopupCozIkiA"
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:icon="@android:drawable/arrow_down_float"
                app:layout_constraintStart_toStartOf="@+id/buttonPopupCozBirA"
                app:layout_constraintTop_toTopOf="@+id/editTextIpikRaporIkiA" />

            <Button
                android:id="@+id/buttonPopupCozUcA"
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:icon="@android:drawable/arrow_down_float"
                app:layout_constraintStart_toStartOf="@+id/buttonPopupCozIkiA"
                app:layout_constraintTop_toTopOf="@+id/editTextIpikRaporUcA"
                tools:ignore="TouchTargetSizeCheck" />

            <Button
                android:id="@+id/buttonPopupCozDortA"
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:icon="@android:drawable/arrow_down_float"
                app:layout_constraintStart_toStartOf="@+id/buttonPopupCozUcA"
                app:layout_constraintTop_toTopOf="@+id/editTextIpikRaporDortA"
                tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck" />

            <Button
                android:id="@+id/buttonPopupCozBesA"
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:icon="@android:drawable/arrow_down_float"
                app:layout_constraintStart_toStartOf="@+id/buttonPopupCozDortA"
                app:layout_constraintTop_toTopOf="@+id/editTextIpikRaporBesA"
                tools:ignore="SpeakableTextPresentCheck" />

            <Button
                android:id="@+id/buttonHesaplaA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:text="Hesapla"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textViewBesIplikA" />

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarAtkiGider"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:background="@drawable/toolbar_bg"
                android:minHeight="?attr/actionBarSize"
                android:theme="?attr/actionBarTheme"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                />

            <EditText
                android:id="@+id/editTextCözgüTarakEniA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:ems="10"
                android:hint="ÇözgüTarakEni"
                android:inputType="number|numberDecimal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/toolbarAtkiGider" />

            <EditText
                android:id="@+id/editTextAtkiPuskulA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:ems="10"
                android:hint="Atkı Püskül"
                android:inputType="number|numberDecimal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextCözgüTarakEniA" />

            <EditText
                android:id="@+id/editTextSiklikA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:ems="10"
                android:hint="Sıklık"
                android:inputType="number|numberDecimal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextAtkiPuskulA" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>